{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "babel-node src/app.js",
    "watch": "npm-watch",
    "lint": "eslint src test",
    "test": "jest"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "npm-watch": "^0.2.0"
  },
  "watch": {
    "start": "src",
    "lint": "{src,test}/*.js",
    "test": "{src,test}/*.js"
  },
  "jest": {
    "testRegex": "/test/.*",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0"
  }
}
